<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>netcoreapp2.2</TargetFramework>
    <Version>3.0.7176.512</Version>
    <Company>Dogus Technology</Company>
    <Authors>Dogus Technology DevOps Team</Authors>
    <Description>Doğuş Teknoloji Site Monitoring Service</Description>
    <Copyright>Copyright ©  2019</Copyright>
    <PackageLicenseFile></PackageLicenseFile>
    <NeutralLanguage>en-US</NeutralLanguage>
    <AssemblyVersion>3.0.7248.7</AssemblyVersion>
    <FileVersion>3.0.7248.7</FileVersion>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <TreatWarningsAsErrors>false</TreatWarningsAsErrors>
    <WarningsAsErrors />
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="DSM.Agents" Version="1.0.7248.6" />
    <PackageReference Include="DSM.Controller" Version="1.0.7248.5" />
    <PackageReference Include="DSM.Core" Version="1.0.7248.13" />
  </ItemGroup>

  <ItemGroup>
    <None Update="appsettings.json">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
  </ItemGroup>

</Project>
